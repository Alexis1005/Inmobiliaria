<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AgenteControlador</servlet-name>
        <servlet-class>com.mycompany.inmobiliaria.controlador.AgenteControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PropiedadesCaracteristicasControlador</servlet-name>
        <servlet-class>com.mycompany.inmobiliaria.controlador.PropiedadesCaracteristicasControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgenteControlador</servlet-name>
        <url-pattern>/AgenteControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PropiedadesCaracteristicasControlador</servlet-name>
        <url-pattern>/PropiedadesCaracteristicasControlador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
